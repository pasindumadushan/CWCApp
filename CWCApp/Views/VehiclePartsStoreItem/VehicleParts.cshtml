                                </table>
                            </div>
                        </div>
                    </div>

                </div>


                <!-- visit modal -->
                <div class='modal fade' id='VisitModal' tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true'>
                    <div class='modal-dialog' role='document'>
                        <div class='modal-content'>
                            <div class='modal-header'>
                                <h5 class='modal-title' id='exampleModalLabel'>Visit Our Store!</h5>
                                <button type='button' class='close' data-dismiss='modal' aria-label='Close'>
                                    <span aria-hidden='true'>&times;</span>
                                </button>
                            </div>
                            <div class='modal-body'>
                                <div class="card mb-4 py-3 border-left-success">
                                    <div class="card-body">
                                        <form Id="VisitForm" novalidate>
                                            <div class="row">
                                                <div class="col-6">
                                                    <div class="form-group">
                                                        <p class="text-secondary">
                                                            Shop :
                                                            <span class="label label-default" id="StoreNameV" name="StoreNameV"></span>
                                                        </p>
                                                    </div>
                                                    <div class="form-group">
                                                        <p class="text-secondary">
                                                            Item :
                                                            <span class="label label-default" id="ItemNameV" name="ItemNameV"></span>
