                                           placeholder="">

                                    <div class="row">
                                        <div class="col-6">
                                            <img src='#' id="ItemImage" width='200px' heigth='200px'>
                                        </div>
                                        <div class="col-6">
                                            <div class="form-group">
                                                <p class="text-secondary">
                                                    Shop :
                                                    <span class="label label-default" id="StoreName" name="StoreName"></span>
                                                </p>
                                            </div>
                                            <div class="form-group">
                                                <p class="text-secondary">
                                                    Item :
                                                    <span class="label label-default" id="ItemName" name="ItemName"></span>
                                                </p>
                                            </div>
                                            <div class="form-group">
                                                <p class="text-secondary">
                                                    Type :
                                                    <span class="label label-default" id="ItemType" name="ItemType"></span>
                                                </p>
                                            </div>
                                            <div class="form-group">
                                                <p class="text-secondary">
                                                    Brand :
                                                    <span class="label label-default" id="ItemBrand" name="ItemBrand"></span>
                                                </p>
                                            </div>
                                            <div class="form-group">
                                                <p class="text-secondary">
                                                    Discription :
                                                    <span class="label label-default" id="ItemDiscription" name="ItemDiscription"></span>
                                                </p>
                                            </div>
                                            <div class="form-group">
                                                <p class="text-secondary">
                                                    Specs :
